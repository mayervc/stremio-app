{
  "expo": {
    "name": "stremio-app",
    "slug": "stremio-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/film_logo.png",
    "scheme": "stremioapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mayer-vargas.stremioapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#000000",
        "foregroundImage": "./assets/images/film_logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mayer_vargas.stremioapp"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/film_logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow stremio-app to access your Face ID biometric data."
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "08f0560a-137d-43cf-9f45-d154954e42d2"
      }
    },
    "owner": "cf-training"
  }
}
